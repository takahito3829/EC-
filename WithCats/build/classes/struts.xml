<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<!-- Strutsの詳細設定 -->
<struts>
<constant name="struts.devMode" value="true" />

<!-- 対象のpackageを設定 -->
<package name="com.internousdev.WithCats.action" extends="struts-default">


<!--   Home画面への遷移(仮ID発行のため) -->
	<action name="TopAction" class="com.internousdev.WithCats.action.TopAction" method="execute">
		<result name="success">home.jsp</result>
	</action>

<!--   お問合わせ画面への遷移 -->
	<action name="InquiryAction" class="com.internousdev.WithCats.action.InquiryAction" method="execute">
		<result name="success">inquiry.jsp</result>
	</action>

	<action name="InquiryConfirmAction" class="com.internousdev.WithCats.action.InquiryConfirmAction" method="execute">
		<result name="success">inquiryConfirm.jsp</result>
		<result name="error">error.jsp</result>
	</action>

	<action name="InquiryCompleteAction" class="com.internousdev.WithCats.action.InquiryCompleteAction" method="execute">
		<result name="success">inquiryComplete.jsp</result>
	</action>





<!--   ユーザー登録画面への遷移 -->
	<action name="UserCreateAction" class="com.internousdev.WithCats.action.UserCreateAction" method="execute">
		<result name="success">userCreate.jsp</result>
	</action>

<!--   ホーム画面への遷移 -->
	<action name="HomeAction" class="com.internousdev.WithCats.action.HomeAction" method="execute">
		<result name="success">home.jsp</result>
	</action>






</package>
</struts>

